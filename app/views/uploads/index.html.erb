<p id="notice"><%= notice %></p>

<h1>Listing Uploads</h1>

<table class="table table-striped">
  <thead class="thead-inverse">
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Size</th>
      <th>URL</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uploads.each do |upload| %>
      <tr>
        <td><%= link_to upload.file_name, upload %></td>
        <td><%= upload.file_type %></td>
        <td><%= upload.file_size %></td>
        <td><%= link_to 'Click Here', upload.file_url %></td>
        <td><%= link_to 'Edit', edit_upload_path(upload), class: "btn btn-primary" %></td>
        <td><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

